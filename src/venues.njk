---
title: Venues
order: 40
eleventyComputed:
  eleventyNavigation:
    key: "{{ title }}"
    order: "{{ order }}"
---
<div class="container my-8 mx-auto prose md:prose-xl">
    <h2 class="mb-2 md:mb-4">{{ title }}</h2>

    <ul class="list-disc pl-4 space-y-2 not-prose">
        {% for venue in collections.venues %}
            <li>
                <a href="{{ venue.page.url }}" class="link">{{ venue.data.title }}</a>
            </li>
        {% endfor %}
    </ul>
</div>
